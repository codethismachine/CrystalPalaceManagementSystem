<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="500.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crystal_palace_management_system.Controller.CheckInController">
   <children>
      <AnchorPane id="ancherPane2" fx:id="checkInSubmit" prefHeight="300.0" prefWidth="500.0" styleClass="top-segment">
         <children>
            <VBox id="VBox" alignment="CENTER" layoutX="36.0" layoutY="2.0" prefHeight="300.0" prefWidth="421.0" spacing="5.0" AnchorPane.leftAnchor="36.0" AnchorPane.rightAnchor="36.0">
               <children>
                  <Label id="userLabel" style="-fx-font-size: 20; -fx-font-weight: bold;" text="Guest Search" />
                  <AnchorPane id="AnchorPane" prefHeight="220.0" prefWidth="428.0">
                     <children>
                        <TextField id="usernameText" fx:id="checkInLastName" layoutX="14.0" layoutY="40.0" prefHeight="31.0" prefWidth="355.0" promptText="Enter Guest Last Name" AnchorPane.leftAnchor="14.0" />
                        <TextField id="usernameText" fx:id="checkInFirst" layoutX="14.0" layoutY="90.0" prefHeight="31.0" prefWidth="355.0" promptText="Enter Guest First Name" />
                        <TextField id="usernameText" fx:id="checkInPhone" layoutX="14.0" layoutY="138.0" prefHeight="31.0" prefWidth="355.0" promptText="Enter Guest Phone Number" />
                        <TextField id="usernameText" fx:id="checkInReservationNumber" layoutX="14.0" layoutY="175.0" prefHeight="31.0" prefWidth="355.0" promptText="Enter Guest Reservation Number" />
                     </children>
                  </AnchorPane>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <Button id="loginButton" fx:id="submitCheckIn" defaultButton="true" layoutX="60.0" layoutY="345.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="50.0" prefWidth="150.0" text="Submit" />
      <Button id="registerButton" fx:id="checkInBack" layoutX="223.0" layoutY="345.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Back" />
   </children>
</AnchorPane>
